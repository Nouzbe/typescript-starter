{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  },
  "rules": {
    "interface-name": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": [true, "dev"],
    "object-literal-sort-keys": false,
    "member-access": false
  }
}
